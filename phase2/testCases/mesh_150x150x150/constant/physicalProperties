FoamFile {
    version 2.0;
    format ascii;
    class dictionary;
    location "constant";
    object physicalProperties;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Thermal diffusivity (aluminum)
alpha alpha [0 2 -1 0 0 0 0] 9.7e-5;

DT DT [0 2 -1 0 0 0 0] 9.7e-5;

// CUDA grid dimensions (must match blockMeshDict)
cudaGridNx 150;
cudaGridNy 150;
cudaGridNz 150;

