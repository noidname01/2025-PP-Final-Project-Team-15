FoamFile
{
    version 2.0;
    format ascii;
    class dictionary;
    location "system";
    object fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Note: These solvers are not used since CUDA handles the evolution
// However, valid dictionaries are required for OpenFOAM to run

solvers
{
    T
    {
        solver PCG;
        preconditioner DIC;
        tolerance 1e-06;
        relTol 1e-06;
    }
}

SIMPLE
{
    nNonOrthogonalCorrectors 0;
}

